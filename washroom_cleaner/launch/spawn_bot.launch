<launch>
  <param
    name="robot_description"
    textfile="$(find washroom_cleaner)/urdf/bot.urdf" />
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />
  <node
    name="urdf_spawner"
    pkg="gazebo_ros"
    type="spawn_model"
    respawn="false"
    output="screen"
    args="-urdf -model toilet_cleaning_bot -param robot_description"/>
  <rosparam
    file="$(find washroom_cleaner)/config/claw_control.yaml"
    command="load"/>
  <node
    name="controller_spawner"
    pkg="controller_manager"
    type="spawner"
    respawn="false"
    output="screen"
    ns="/washroom_cleaner"
    args="claw_controller joint_state_controller"/>
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher"
    respawn="false"
    output="screen">
    <remap
      from="/joint_states"
      to="/washroom_cleaner/joint_states" />
  </node>
</launch>
